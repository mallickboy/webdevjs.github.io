extends base.pug

block scripts  
    script(src="../static/script.js") 
block style 
    style 
        include ../static/style.css
        include ../static/styleContact.css
block content 
    div.container 
        h1 Contact Us
        form(action="/contact" ,method="post",class="myForm")
            input(type="text", name="name",class="myInput" placeholder="Enter your name") 
            input(type="number", name="age",class="myInput" placeholder="Enter your age") 
            input(type="text", name="gender",class="myInput" placeholder="Enter your gender") 
            input(type="phone", name="phone",class="myInput" placeholder="Enter your phone number") 
            input(type="email", name="email",class="myInput" placeholder="Enter your email id") 
            input(type="text", name="address",class="myInput" placeholder="Enter your address") 
            button(class="btn")
                |Submit
            
